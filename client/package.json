  "description": "Etheremon Client App",
  "main": "app.jsx",
  "scripts": {
    "startDev": "del-cli public/* && webpack-dev-server --port 8080 --content-base=public --hot --watch-poll --config webpack.dev.js --env.ENV=dev",
    "startProd": "del-cli public/* && webpack-dev-server --port 8080 --content-base=public --hot --watch-poll --config webpack.dev.js --env.ENV=prod",
    "buildDev": "del-cli public/* && webpack -p --config webpack.prod.js --env.ENV=dev",
    "buildProd": "del-cli public/* && webpack -p --config webpack.prod.js --env.ENV=prod"
  },
  "keywords": [
    "emont",
    "decentralize",
    "ethereum"
